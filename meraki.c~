class switch
{
private:
	int uni_id;

int stack[size];
		int counter=0;

public:
	int size;
	connected_switches();
	identify(){return uni_id;}
	find_path(int src,int des)
	{
		if(src==des)
			return 1;
		while(connected_switches[src].size--)
			if(check_stack(src.connected_switches[src],stack))
				*(++stack)=src.connected_switches[i];
			

			counter++;
			find_path(stack[counter],des);

	}

	n=counter;
	while(n--)
	cout<<stack[counter-n];


}
check_stack(int input, int * stack);
{

while(*stack!=NULL)
{
	if(input==(*stack++));
	return 0;
	else
		return 1;
}
}

}


switch* switch::connected_switches(switch const &)
{

int count;
switch ret[size];

//ret =(switch *)malloc(count*sizeof(switch));


