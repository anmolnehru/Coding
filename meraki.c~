{
	if(connections[switch_object][i]>0)
		//cout<<switch.i<<endl; //this would output the switch objects connected to the inputted object
		ret[i]=switch.identify;
		count++
}

return ret;




int find_path(int src,int des)
{

int index=1;

stack[0]=src;

if(i==des)
	return 1;
for(int i=0;i<size;i++)
